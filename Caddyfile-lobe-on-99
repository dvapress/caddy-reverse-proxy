

# 将所有请求代理到 https://lobe.xxx.bbb:9443
lobed.dva.autos {  
    reverse_proxy https://lobe.dva.autos:9443 {
        # 可选：关闭SSL验证（若目标使用自签名证书）
        transport https {
            # tls
            tls_insecure_skip_verify
        }
        # header_up Host lobe.dva.autos
        # header_up X-Forwarded-Proto https
    }
}

